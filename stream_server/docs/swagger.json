                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/definitions/dto.VideoIndexOutput"
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
